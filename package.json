        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "no"
  },
  "keywords": [
    "node"
  ],
  "author": "sumeet",
  "license": "ISC",
  "dependencies": {
    "@jest/test-sequencer": "^26.6.3",
    "@shelf/jest-mongodb": "^1.2.3",
    "accesscontrol": "^2.2.1",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "bootstrap": "^5.0.0-beta1",
    "bson-objectid": "^1.3.1",
    "config": "^3.3.3",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "jest": "^26.6.3",
    "jest-stare": "^2.2.0",
    "jquery": "^3.5.1",
